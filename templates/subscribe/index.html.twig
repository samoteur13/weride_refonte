{% extends 'base.html.twig' %}

{% block title %}Hello SubscribeController!
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">

		{# {{ form_start(form) }}
		<div>
			{{ form_row(form.email, {'label': 'Votre Email'}) }}
		</div> #}
		{{ form_start(form) }}
		{{ form_errors(form) }}

		{{ form_row(form.email) }}
		{{ form_row(form.lastname) }}
        {{ form_row(form.firstname) }}
        {{ form_row(form.pseudo) }}
        {{ form_row(form.avatar) }}
        {{ form_row(form.pictures) }}
        {{ form_row(form.password) }}
        <input type="submit" >
		{{ form_end(form, {'render_rest': false}) }}

	</div>
{% endblock %}
